{
  "dependencies": {
    "@assemblyscript/loader": "^0.18.16",
    "assemblyscript": "^0.18.17",
    "ws": "^7.4.4"
  },
  "scripts": {
    "asbuild:untouched": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts --target debug --runtime incremental --exportRuntime --exportTable",
    "asbuild:optimized": "asc ./node_modules/as-bind/lib/assembly/as-bind.ts assembly/index.ts --target release --runtime incremental --exportRuntime --exportTable",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests",
    "start": "node server.js"
  },
  "devDependencies": {
    "assemblyscript": "^0.18.16"
  },
  "name": "as-ws",
  "description": "WebSocket bindings for AssemblyScript",
  "version": "1.0.0",
  "types": "./assembly/index.ts",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JairusSW/as-ws.git"
  },
  "keywords": [
    "WebSocket",
    "assemblyscript",
    "socket",
    "bindings",
    "ws"
  ],
  "author": "JairusSW",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JairusSW/as-ws/issues"
  },
  "homepage": "https://github.com/JairusSW/as-ws#readme"
}
